{% extends "base.html.j2" %}


{% block body %}

{% include "examples.html.j2" %}

{% include "repl.html.j2" %}


<h2>&gt;&gt; {{repl.statement}}</h2>

{% match error %}
{% when Some with (e) %}
<strong>Error</strong><br>
<p class="repl-output">{{ e }}</p>
{% when None %}
{% endmatch %}

{% match output %}
{% when Some with (e) %}
<strong>Output</strong><br>
<p class="repl-output">{{ e }}</p>
{% when None %}
{% endmatch %}

{% endblock body %}